# GraphQL schema example
#
# https://gqlgen.com/getting-started/

scalar UUID

type User {
  Id: Int!
  username: String!
  email: String!
  password: String!
}

type Query {
  users: [User!]!
}

input NewUser {
  username: String!
  email: String!
  password: String!
}

type Mutation {
  signup(input: NewUser!): User!
}
