# GraphQL schema example
#
# https://gqlgen.com/getting-started/


type User {
  id: ID!
  username: String!
  email: String!
  password: String!
}

type Query {
  users: [User!]!
}

input NewTodo {
  name: String!
  email: String!
  password: String!
}

type Mutation {
  signup(input: NewTodo!): User!
}
